@model IEnumerable<ChemistShopSite.Models.User>

@{
    ViewData["Title"] = "UserList";
}

<h2>Users List</h2>

<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td >
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
            </td>
        </tr>
    }
</table>